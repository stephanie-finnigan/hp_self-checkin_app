@using SelfCheckinWebApp.Enums
@{
    ViewBag.Title = "_EndCheckInProcessPartial";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var site = ViewBag.site;

    var featureSwitchValues = ViewBag.featureSwitchValues;
    var incorrDetailsMsgVal = featureSwitchValues[FeatureSwitchEnum.IncorrectDetailsMsg];
}

<div class="row-end-process">
    <p>
        @{
            string[] result = IncorrDetailsMsgVal.Split(new string[] { "." }, StringSplitOptions.RemoveEmptyEntries);

            foreach (string str in result)
            {
                @(str + ".")
                <br />
            }
        } 
    </p>
</div>
<br />
<div class="btn-exit">
    @Html.ActionLink("Return to Find My Booking", "Find", "CheckIn", new { @class = "btn btn-primary" })
</div>
<br />
<br />
<div class="row">
    <img class="branding-image" src="~/Content/@site/logo.svg" />
</div>